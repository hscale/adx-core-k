{
  "name": "adx-core-frontend",
  "version": "1.0.0",
  "description": "ADX CORE Universal Frontend - Web, Desktop & Mobile",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:web": "vite --mode web",
    "dev:desktop": "vite --mode desktop",
    "dev:mobile": "vite --mode mobile",
    "build": "tsc && vite build",
    "build:web": "tsc && vite build --mode web",
    "build:desktop": "tsc && vite build --mode desktop",
    "build:mobile": "tsc && vite build --mode mobile",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "tauri:build:desktop": "tauri build --target desktop",
    "tauri:build:mobile": "tauri build --target mobile"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^3.3.2",
    "@tanstack/react-query": "^5.8.4",
    "@tanstack/react-query-devtools": "^5.84.1",
    "@tanstack/react-table": "^8.10.7",
    "@tauri-apps/api": "^2.0.0-beta.0",
    "@tauri-apps/plugin-dialog": "^2.0.0-beta.0",
    "@tauri-apps/plugin-fs": "^2.0.0-beta.0",
    "@tauri-apps/plugin-notification": "^2.0.0-beta.0",
    "@tauri-apps/plugin-shell": "^2.0.0-beta.0",
    "axios": "^1.6.2",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "framer-motion": "^10.16.5",
    "i18next": "^23.7.6",
    "i18next-browser-languagedetector": "^7.2.0",
    "lucide-react": "^0.294.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "react-hot-toast": "^2.4.1",
    "react-i18next": "^13.5.0",
    "react-router-dom": "^6.20.1",
    "tailwind-merge": "^2.0.0",
    "zod": "^3.22.4",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2.0.0-beta.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.1.1",
    "@vitest/ui": "^0.34.6",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vitest": "^0.34.6"
  }
}
